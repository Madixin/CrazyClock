{
  "app": {
    "bundleName": "com.madixin.codelab",
    "vendor": "madixin",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.madixin.clock.clock",
    "name": ".MyApplication",
    "deviceType": [
      "phone",
      "tablet"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "clock",
      "moduleType": "feature",
      "installationFree": true
    },
    "abilities": [
      {
        "orientation": "unspecified",
        "visible": true,
        "name": "com.madixin.clock.clock.ClockAlarmAbility",
        "icon": "$media:icon",
        "description": "$string:clockalarmability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ]
  }
}